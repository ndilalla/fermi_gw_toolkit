    <task name="FTI_Likelihood"
          version="1.6" 
          type="Data">
      <notation>
        Perform a likelihood analysis and compute bayesian upper limits
      </notation>
      <prerequisites>
        <prerequisite name="OBJ_RA" type="string"/>
        <prerequisite name="OBJ_DEC" type="string"/>
